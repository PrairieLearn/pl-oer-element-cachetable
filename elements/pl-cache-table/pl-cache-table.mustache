                        {{/show_dirty}}
                        {{#block_list}}
                            <th>Off {{offset}}</th>
                        {{/block_list}}
                    {{/way_list}}
                </tr>                    
            </thead>
            <tbody>
                {{#cache_sets}}
                    <tr>
                        {{^fully_assoc}}<td>{{index}}</td>{{/fully_assoc}}
