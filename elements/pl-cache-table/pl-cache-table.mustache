            {{/all_correct}}
            {{#all_incorrect}}
                <span class="badge badge-danger badge-large"><i class="fa fa-times-circle" aria-hidden="true"></i> 0%</span>
            {{/all_incorrect}}
            {{#all_partial}}
                {{#score}}<span class="badge badge-warning badge-large">{{score}}%</span>{{/score}}
            {{/all_partial}}
        </span>
        {{/show-percentage-score}}
        <table class="cache-table table table-hover table-striped table-condensed overflow-hidden" {{#blocks}}style="border-collapse:separate"{{/blocks}} border="1">
